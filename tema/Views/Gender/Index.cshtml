@model List<Gender>

<h1>Gender List</h1>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Gjinia</th>
        <th>Gjinia Anglisht</th>
        <th>Gjinia Serbisht</th>
    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.AlDescription</td>
                <td>@obj.EnDescription</td>
                <td>@obj.SrDescription</td>
            </tr>
        }
    </tbody>
</table>
