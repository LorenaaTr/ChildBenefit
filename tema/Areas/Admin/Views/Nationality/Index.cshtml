@model List<Nationality>

<div class="card shadow border-0 mt-4 content">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-primary">Lista Nacionaliteteve</h2>
            </div>
        </div>
    </div>
    <div class="card-body p-4">
        <div class="row  pb-2 ">
            <div class="col-6">
            </div>
            <div class="col-6 text-end">
                <a asp-controller="Nationality" asp-action="Create" class="btn btn-primary">
                    <i class="bi bi-plus-circle"></i> Shto Nacionalitet
                </a>
            </div>
        </div>
        <table id="tblData" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Nacionaliteti</th>
                    <th>Nacionaliteti Anglisht</th>
                    <th>Nacionaliteti Serbisht</th>
                </tr>
            </thead>
            <tbody>

            </tbody>
        </table>
    </div>
</div>

@section Scripts {
    <script src="~/js/nationality.js"></script>
}
